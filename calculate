index.js


function Calculator(event) {
  event.preventDefault();
  const mul = document.getElementById("firstno").value;
  const mul2 = document.getElementById("secondno").value;
  console.log(Number(mul)*Number(mul2))
}

index.html<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <input id="firstno" type="number" />
    <input id="secondno " type="number" />
    <button onclick="Calculator(event)">calculate</button>
    <h3></h3>
  </body>
  <script src="index.js"></script>
</html>


